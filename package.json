{
  "name": "obsidian-aloud-tts",
  "version": "0.9.0",
  "description": "Highlight and speak text from your obsidian notes",
  "main": "main.js",
  "scripts": {
    "all": "pnpm run lint; pnpm run format:check; pnpm run test run; pnpm run build",
    "dev": "node esbuild.config.mjs",
    "typecheck": "tsc -noEmit -skipLibCheck",
    "build": "pnpm run typecheck && node esbuild.config.mjs production",
    "version": "./version-bump.sh",
    "lint": "eslint src",
    "test": "vitest",
    "format:check": "prettier --check src",
    "format": "prettier --write src",
    "web:dev": "vite src/web/",
    "web:build": "vite build src/web/",
    "prepare": "husky"
  },
  "keywords": [
    "obsidian",
    "text-to-speech"
  ],
  "author": "Adrian Lyjak",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adrianlyjak/obsidian-aloud-tts"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.16.10",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "@vitest/coverage-v8": "3.2.4",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.4",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.11",
    "obsidian": "^1.7.2",
    "prettier": "^3.3.3",
    "tslib": "2.6.2",
    "typescript": "5.3.2",
    "vite": "^6.3.5",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "@breezystack/lamejs": "^1.2.7",
    "@codemirror/language": "^6.10.3",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.34.1",
    "@google/genai": "^1.5.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "fft.js": "^4.0.4",
    "idb": "^8.0.0",
    "mobx": "^6.13.3",
    "mobx-react-lite": "^4.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "packageManager": "pnpm@9.0.2+sha1.2a34aeb80bf8e8435815d14f620ec4ba0eff1604",
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --check",
      "eslint"
    ]
  },
  "pnpm": {
    "overrides": {
      "cross-spawn@>=7.0.0 <7.0.5": ">=7.0.5",
      "nanoid@<3.3.8": ">=3.3.8",
      "elliptic@<6.6.0": ">=6.6.0",
      "esbuild@<=0.24.2": ">=0.25.0",
      "vitest@>=1.0.0 <1.6.1": ">=1.6.1",
      "vite@>=5.0.0 <=5.4.11": ">=5.4.12",
      "elliptic@<=6.6.0": ">=6.6.1",
      "vite@>=5.0.0 <5.4.15": ">=5.4.15"
    }
  }
}