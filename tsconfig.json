{
  "compilerOptions": {
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "ESNext",
    "target": "ES6",
    "allowJs": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "jsx": "react",
    "lib": ["DOM", "ES5", "ES6", "ES7"]
  },
  "include": ["**/*.ts", "src/Plugin.tsx", "src/web/app.tsx"]
}
